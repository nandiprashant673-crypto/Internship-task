# Program to print personal details

name = "Prashant"
age = 23
course = "MCA"
college = "KLECET Chikodi"
email = "prashantnandi3@gmail.com"

print("----- Personal Details -----")
print("Name   :", name)
print("Age    :", age)
print("Course :", course)
print("College:", college)
print("Email  :", email)
